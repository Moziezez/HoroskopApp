@keyframes pop-in {
	from { background-color: rgba(174, 134, 69, 0.703); opacity: 0.1;}
	to { background-color: rgb(255, 255, 255); }
}

@media only screen and (max-width: 423px) {
	.border {
		width: 100% !important;
	}
}

.coords-container {
	display: flex;
	flex-direction: row;
	align-items: center;
}
input[type=text].coord-input {
	width: 46px !important;
	text-align: end;
}

div.text-container {
	height: auto;
	margin: 5px 5.5px;
	text-align: justify; 
	box-sizing: border-box;
	border-style: outset;
	border-color: lightgray;
	border-radius: 3px;
	border-width: 0;
	padding: 19px; 
	opacity: 1;
	animation-name: pop-in;
	animation-duration: 1s;
	animation-timing-function: ease-out;
	font-family: Iowan Old Style, Palatino, "Palatino Linotype", "Book Antiqua", "URW Palladio L", P052, serif !important;
	background-color: white;
	color: black !important;
}
.border{
	border: 1.5px solid #fcd9a9;
	border-radius: 5px;
	width: 401px;
	height: auto;
	margin: 12px auto;
}
#checkbox {
	margin-left: 22px;
}
input[type=checkbox].aspect-check {
	width: 14px !important;
	height: 14px !important;
}
input[type=checkbox]#che {
	width: 14px !important;
	height: 14px !important;	
}
.grid-item {
	position: relative;
	margin-bottom: 24px;
}
.dotted-line {   
	position: relative;
	width: 100%;
	height: 1px;
	background-image: linear-gradient(to right, #fcd9a9 50%, transparent 50%);
	background-size: 2px 2px; /* space between dots */
	background-repeat: repeat-x;
}

input.horo-inpfield, input.geoapify-autocomplete-input {
	border: none !important;
	outline: none !important;
	height: 29px !important;
	width: 100% !important;
	margin: 0 !important;
	padding: 0 1px 0 0 !important;
	background-color: white !important;
	color: black !important;
	font-size: 10pt !important;
	min-height: auto !important;
}
input#dat::placeholder, input#nam::placeholder, input#hour::placeholder, input#minu::placeholder, input#lon::placeholder, input#lat::placeholder, input[type=text].geoapify-autocomplete-input::placeholder {
	color: rgba(121, 121, 121, 0.741) !important;
	font-size: 10pt !important;
	margin: 0 !important;
	padding: 0 !important;
}
input[type=text].geoapify-autocomplete-input {
	color: black;
	font-size: 10pt !important;
}
.autocomplete-container {
	/*the container must be positioned relative: input.geoapify-autocomplete-input*/
	position: relative;
	width: 100%;
}
.geoapify-autocomplete-items {
	position: absolute;
	border: 1px solid rgba(0, 0, 0, 0.3);
	border-top-width: 1px;
	border-top-style: solid;
	border-top-color: rgba(0, 0, 0, 0.3);
	border-top: none;
	border-radius: 5px;
	background-color: #fff;
	z-index: 99;
	top: 100%;
	left: 0;
	right: 0;
	width: 100%; 
	cursor: pointer;
}
.geoapify-autocomplete-item {
	padding: 6px !important;
	margin: 1px;
	display: flex;
}
span.address>span.secondary-part {
	margin-left: 22px;
}
label {
	display: block;
	margin-bottom: 4px;
}	
span.marTop{
	display: block;
	margin-top: 12px;
}
span.sublabel {
	font-family: "Arial";
	font-size: 9px;
	color: black;
}
span.sublabel.bold {
	font-weight: bold;
	font-size: 9px;
}
span.address>span.min-part {
	color:rgba(219, 49, 49, 0.877);
}
.geoapify-autocomplete-input {
	display: flex;
	flex-direction: row;
	align-items: center;
	height: 31.5px !important;
}
.geoapify-close-button.visible {
	display: flex;
}
.geoapify-close-button {
	cursor: pointer;
	position: absolute;
	right: 5px;
	top: 0;
	height: 100%;
	align-items: center;
	visibility: collapse;
}
.geoapify-autocomplete-item .icon {
	padding: 0 6px;
	display: inline-block;
	width: 27px;
	height: 27px;
}
.geoapify-autocomplete-item .icon > svg {
	width: 14px;
}
#but {
	color: rgb(255, 255, 255);
	background-color: rgb(252, 169, 45);
	width: 100%;
	border-radius: 5px;
	border-width: 1px;
	cursor: pointer;
}
#House {
	width: 131px;
	border-radius: 2px;	
	border-width: 1.5px;
	border-color: orange;
	background-color: white;
	margin-left: 3px;
	text-align: center;
	padding: 0 !important;
}
option {
	padding: 8px 2px;
}
.input-time {
	width: 6.6%;
}
#minu{
	text-align: left;
}
#hour{
	text-align: center;
}
.flex-time {
	display: flex;
	align-items: center;
}
.flex-radio {
	display: flex;
	flex-wrap: wrap;
	gap: 14px;
}
#signsSystem {
	display: flex;
	gap: 14px;
}
span.ddot-time {
	color: black;
}
iframe {
	height: 234px;
}
